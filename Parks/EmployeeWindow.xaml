<Window x:Class="Parks.EmployeeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parks"
        mc:Ignorable="d"
        Title="EmployeeWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn_back" Content="Назад" FontSize="30" Click="btn_back_Click" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBlock x:Name="tb_employee_name" Text="Ты пирожок" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
